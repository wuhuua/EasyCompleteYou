    },
    "formatting": {
      "dynamicRegistration": false
    },
    "rangeFormatting": {
      "dynamicRegistration": false
    },
    "onTypeFormatting": {
      "dynamicRegistration": false
    },
    "declaration": {
      "dynamicRegistration": false,
      "linkSupport": true
    },
    "definition": {
      "dynamicRegistration": false,
      "linkSupport": true
    },
