{
  "GLOBAL_SETTING": {
    "lsp_formatting": {
      "default_value": {
        "tabSize": 2,
        "insertSpaces": true,
        "trimTrailingWhitespace": true,
        "trimFinalNewlines": true,
        "insertFinalNewline": true
      },
      "type": "dict",
      "des": ["lsp_formatting opts"]
    },
    "lsp_timeout": {
      "default_value": 5,
      "type": "",
      "des": ["timeout after request."]
    }
  },
  "ECY_engines.cpp.clangd.clangd": {
    "cmd": {
      "default_value": "clangd",
      "type": "string",
      "des": ["cmd to start clangd."]
    },
    "compileFallback": {
      "default_value": [],
      "type": "list",
      "des": ["flag to be added while analysing."]
    }
  },
  "ECY_engines.python.pyright.pyright": {
    "cmd": {
      "default_value": "pyright-langserver --stdio",
      "type": "string",
      "des": ["cmd to start pyright."]
    },
    "include_path": {
      "default_value": [],
      "type": "list",
      "des": ["included path while parsing."]
    }
  },
  "ECY_engines.python.pyls.pyls": {
    "cmd": {
      "default_value": "jedi-language-server",
      "type": "string",
      "des": ["cmd to start pyls."]
    },
    "initializationOptions": {
      "default_value": {
        "codeAction": {
          "nameExtractVariable": "jls_extract_var",
          "nameExtractFunction": "jls_extract_def"
        },
        "completion": {
          "disableSnippets": false,
          "resolveEagerly": false
        },
        "diagnostics": {
          "enable": true,
          "didOpen": true,
          "didChange": true,
          "didSave": true
        },
        "jediSettings": {
          "autoImportModules": [],
          "caseInsensitiveCompletion": true
        },
        "markupKindPreferred": "markdown",
        "workspace": {
          "extraPaths": [],
          "symbols": {
            "ignoreFolders": [".nox", ".tox", ".venv", "__pycache__", "venv"],
            "maxSymbols": 20
          }
        }
      },
      "type": "dict",
      "des": ["init opts"]
    }
  }
}
