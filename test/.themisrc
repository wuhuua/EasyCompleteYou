let g:repo_root = fnamemodify(expand('<sfile>'), ':h:h')

call themis#helper('command').with(themis#helper('assert'))

if $PROFILE_LOG !=# ''
    execute 'profile' 'start' $PROFILE_LOG
    execute 'profile!' 'file' g:repo_root . '/autoload/ECY2_main.vim'
    execute 'profile!' 'file' g:repo_root . '/autoload/ECY/*'
endif

call themis#option('runtimepath', expand(g:repo_root))

set encoding=utf-8
set termencoding=utf-8
set fileencoding=utf-8
scriptencoding utf-8
